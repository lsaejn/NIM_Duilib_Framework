        <Button class="btn_wnd_restore" name="restorebtn" visible="false" valign="center" tooltiptext="向下还原"/>
      </Box>
      <Button class="btn_wnd_close" name="closebtn" margin="4,0,4,0"  valign="center" tooltiptext="关闭"/>
    </HBox>
